{
  "plugins": [
    ["transform-react-jsx", { "pragma":"h" }],
    ["module-resolver", {
    "root": ["."],
    "alias": {
        "react": "preact-compat",
        "react-dom": "preact-compat",
        // Not necessary unless you consume a module using `createClass`
        "create-react-class": "preact-compat/lib/create-react-class"
    }
    }]
  ],
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"]
      },
	  "modules": false
    }], 
    "react", 
    "stage-0"
  ]
}
