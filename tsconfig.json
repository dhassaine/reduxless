{
  "compilerOptions": {
    "allowJs": true,
    "target": "es2018",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "jsx": "react"
  },
  "typeAcquisition": {
    "enable": true
  }
}
